{
  "name": "matancohen365/r2l",
  "description": "LTR to RTL Converter - Allows you to take a frontend template designed for left-to-right usage and transform it into right-to-left design.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Matan Cohen",
      "email": "matancohen365@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-dom": "*",
    "intervention/image": "^2.5"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
